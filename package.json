{
  "name": "BOOMBOLT",
  "version": "1.0.0",
  "description": "MERN shopping cart app",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node backend/server",
    "server": "nodemon backend/server",
    "client": "npm start --prefix frontend",
    "admin": "npm start --prefix adminend",
    "dev": "concurrently \"npm run server\" \"npm run client\"  \"npm run admin\"",
    "data:import": "node backend/seeder",
    "data:destroy": "node backend/seeder -d",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix frontend && npm run build --prefix frontend"
  },
  "author": "Ankit Chauhan",
  "license": "MIT",
  "dependencies": {
    "@stripe/react-stripe-js": "^1.3.0",
    "@stripe/stripe-js": "^1.13.1",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cloudinary": "^1.24.0",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-async-handler": "^1.1.4",
    "express-fileupload": "^1.2.1",
    "fast-xml-parser": "^3.19.0",
    "file-reader": "^1.1.1",
    "fs": "^0.0.1-security",
    "grandjs": "^2.2.25",
    "html-pdf": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.10.6",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemailer": "^6.6.2",
    "razorpay": "^2.0.6",
    "react-alert": "^7.0.2",
    "react-facebook-login": "^4.1.1",
    "react-google-login": "^5.2.2",
    "react-zoom-slider": "^0.1.0",
    "stripe": "^8.144.0",
    "underscore": "^1.12.0"
  },
  "devDependencies": {
    "concurrently": "^5.3.0",
    "nodemon": "^2.0.4"
  }
}
